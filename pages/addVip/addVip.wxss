.section {
  height: 100rpx;
  margin: 0 24rpx;
  border-bottom: 1rpx solid #dddddd;
}

.section_select {
  background-color: white;
  margin-bottom: 20rpx;
  border-radius: 4rpx;
  overflow: hidden;
}

.section__title {
  float: left;
  width: 106rpx;
  line-height: 100rpx;
  margin-left: 20rpx;
  color: #999999;
  font-size: 30rpx;
}

.section_input {
  width: 570rpx;
  float: left;
  line-height: 100rpx;
  height: 100rpx;
  color: #333333;
  font-size: 30rpx;
}

.placeholder {
  color: #999999;
}

.disabled {
  color: #666666;
}

.success {
  color: #3669fe;
}

.error {
  color: #ff4141;
}

