			</view>
		</view>
	</view>
	<view class="nav">
		<view wx:for="{{iconList}}" wx:key="index" data-id="{{index}}" class="nav-item" bindtap="bindLink">
			<view class="nav-item-box">
				<image class="nav-item-icon" mode="heightFix" src="{{item.src}}"></image>
			</view>
			<view class="nav-item-text">
				{{item.text}}
			</view>
		</view>
	</view>
